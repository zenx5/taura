{
  "name": "kommiter-ia",
  "version": "1.1.2",
  "type": "module",
  "description": "Crea tus commit con IA y escoge con que IA quieres trabajar",
  "bin": {
    "kommit": "./dist/index.js"
  },
  "scripts": {
    "start": "node --loader ts-node/esm ./src/index.ts",
    "build": "tsc && tsc-alias",
    "migration:generate": "prisma migrate dev --name",
    "postinstall": "npx prisma migrate dev --name auto"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/google": "0.0.39",
    "@ai-sdk/openai": "0.0.48",
    "@prisma/client": "5.18.0",
    "ai": "3.3.9",
    "chalk": "5.3.0",
    "terminal-i2": "1.1.0"
  },
  "devDependencies": {
    "@types/node": "22.4.0",
    "prisma": "5.18.0",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.10",
    "typescript": "5.5.4"
  }
}
